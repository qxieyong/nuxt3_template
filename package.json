{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env.development -- nuxt dev",
    "dev:testing": "cross-env NODE_ENV=testing dotenv -e .env.testing -- nuxt dev",
    "build": "cross-env NODE_ENV=production dotenv -e .env.production -- nuxt build",
    "start": "cross-env NODE_ENV=production dotenv -e .env.production -- nuxt start",
    "generate": "cross-env NODE_ENV=production dotenv -e .env.production -- nuxt generate",
    "preview": "cross-env NODE_ENV=production dotenv -e .env.production -- nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxtjs/i18n": "^9.5.4",
    "@pinia/nuxt": "^0.11.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "lodash-unified": "^1.0.3",
    "nuxt": "^3.17.4",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.2",
    "@tailwindcss/postcss": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "postcss": "^8.5.4",
    "sass": "^1.89.0",
    "tailwindcss": "^4.1.8"
  }
}
